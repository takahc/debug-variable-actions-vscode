{
    "name": "debug-variable-actions-vscode",
    "displayName": "debug-variable-actions-vscode",
    "description": "A VSCode extension for debugging variables",
    "version": "0.0.34",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.89.0"
    },
    "categories": [
        "Debuggers",
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished",
        "onDebug"
    ],
    "publisher": "akipg",
    "repository": {
        "type": "git",
        "url": "https://github.com/takahc/debug-variable-actions-vscode.git"
    },
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "debug-variable-actions.start",
                "title": "Start"
            },
            {
                "command": "debug-variable-actions.action",
                "title": "Action"
            },
            {
                "command": "debug-variable-actions.tocsv",
                "title": "To CSV"
            },
            {
                "command": "debug-variable-actions.topng",
                "title": "To PNG"
            },
            {
                "command": "debug-variable-actions.topanel",
                "title": "To Panel"
            },
            {
                "command": "debug-variable-actions.autocontinueframe",
                "title": "DebugVariableActions: Auto continue for frame"
            },
            {
                "command": "debug-variable-actions.autocontinueframe-stop",
                "title": "DebugVariableActions: Stop auto continue for frame"
            }
        ],
        "menus": {
            "debug/variables/context": [
                {
                    "command": "debug-variable-actions.action",
                    "when": "true"
                },
                {
                    "command": "debug-variable-actions.tocsv",
                    "when": "true"
                },
                {
                    "command": "debug-variable-actions.topng",
                    "when": "true"
                },
                {
                    "command": "debug-variable-actions.topanel",
                    "when": "true"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "debug-variable-actions-activity-bar",
                    "title": "Debug Variable Actions",
                    "icon": "images/icon.png"
                }
            ]
        },
        "views": {
            "debug-variable-actions-activity-bar": [
                {
                    "id": "debug-variable-actions.views.controller",
                    "type": "webview",
                    "name": "Controller",
                    "canToggleVisibility": false
                },
                {
                    "id": "debug-variable-actions.views.image-panel",
                    "type": "webview",
                    "name": "Image Panel",
                    "canToggleVisibility": false
                }
            ]
        },
        "configuration": {
            "title": "Debug Variable Actions",
            "properties": {
                "debug-variable-actions.config.enable": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable the extension"
                },
                "debug-variable-actions.config.prevent-drilldown-conditions": {
                    "type": "array",
                    "description": "Prevent drilldown types",
                    "items": {
                        "type": "string"
                    }
                },
                "debug-variable-actions.config.render-mode": {
                    "type": "string",
                    "default": "panel",
                    "description": "Render mode",
                    "enum": [
                        "panel",
                        "panel-vue",
                        "stack-vue"
                    ],
                    "enumDescriptions": [
                        "Simple visualization",
                        "Stack trace visualization"
                    ]
                },
                "debug-variable-actions.config.image-sizebyte-limit": {
                    "type": "number",
                    "default": 4294967295,
                    "description": "Image size limit (in bytes)"
                },
                "debug-variable-actions.config.post-server": {
                    "type": "boolean",
                    "default": true,
                    "description": "Post messages to the frontend server"
                },
                "debug-variable-actions.config.serv-url": {
                    "type": "string",
                    "default": "http://localhost:8080",
                    "description": "Frontend server URL"
                },
                "debug-variable-actions.config.image-types": {
                    "type": "array",
                    "additionalItems": true,
                    "description": "Additional image types",
                    "items": {
                        "type": "object",
                        "additionalProperties": false,
                        "properties": {
                            "display_name": {
                                "type": "string"
                            },
                            "match_types": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "binary_info": {
                                "type": "object",
                                "additionalProperties": false,
                                "properties": {
                                    "sizeByte": {
                                        "type": "string"
                                    },
                                    "littleEndian": {
                                        "type": "string"
                                    },
                                    "signed": {
                                        "type": "string"
                                    },
                                    "fixedSize": {
                                        "type": "string",
                                        "default": "false"
                                    },
                                    "isInt": {
                                        "type": "string"
                                    }
                                }
                            },
                            "image_info": {
                                "additionalProperties": false,
                                "type": "object",
                                "properties": {
                                    "mem_width": {
                                        "type": "string"
                                    },
                                    "mem_height": {
                                        "type": "string"
                                    },
                                    "image_width": {
                                        "type": "string"
                                    },
                                    "image_height": {
                                        "type": "string"
                                    },
                                    "stride": {
                                        "type": "string"
                                    },
                                    "channels": {
                                        "type": "string"
                                    },
                                    "data": {
                                        "type": "string"
                                    },
                                    "format": {
                                        "type": "string"
                                    },
                                    "bytesForPx": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/ejs": "^3.1.5",
        "@types/mocha": "^10.0.6",
        "@types/node": "18.x",
        "@types/vscode": "^1.89.0",
        "@types/ws": "^8.5.12",
        "@typescript-eslint/eslint-plugin": "^7.11.0",
        "@typescript-eslint/parser": "^7.11.0",
        "@vscode/test-cli": "^0.0.9",
        "@vscode/test-electron": "^2.4.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.57.0",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "dedent": "^1.5.3",
        "ejs": "^3.1.10",
        "jimp": "^0.22.12",
        "optional": "^0.1.4",
        "sharp": "^0.33.5",
        "ws": "^8.18.0"
    }
}
