{
    "debug-variable-actions.config.render-mode": "panel-vue",
    "debug-variable-actions.config.enable": true,
    "debug-variable-actions.config.image-types": [
        {
            "display_name": "MyImage",
            "match_types": ["MyImage", "MyImage *"],
            "binary_info": {
                "sizeByte": "width*height*1*1",
                "littleEndian": "true",
                "signed": "false",
                "fixedSize": "false",
                "isInt": "true"
            },
            "image_info": {
                "mem_width": "width",
                "mem_height": "height",
                "image_width": "width",
                "image_height": "height",
                "stride": "width*1",
                "channels": "1",
                "data": "String(((d)=> data.split(' ')[0] )(data))",
                "format": "'GRAY'",
                "bytesForPx": "1"
            }
        }
    ]
}
